[program:gunicorn]
command={{ virtualenv_path }}/bin/gunicorn -c {{ application_path }}/gunicorn_config.py wsgi
directory={{ application_path }}
environment=PATH="{{ virtualenv_path }}/bin"
user={{ user_name }}
group={{ user_name }}
autorestart=true
autostart={{ autostart | bool | lower }}
redirect_stderr=true
stopasgroup=true
